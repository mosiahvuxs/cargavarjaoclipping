	<queryString>
		<![CDATA[SELECT
       w.conteudo as html, m.data_cadastro, s.descricao, v.descricao as fonte, w.url, m.titulo
from midias m, web w, secoes s, veiculos v

where
     m.id = w.midia_id and tipo_midia_id = 4
     and m.secao_id = s.id and s.veiculo_id = v.id
	and m.id = 973720]]>
